@model IList<IdentityServer.Models.UserViewModels.UserSelectedViewModel>

<table>
    @for(int i = 0; i < Model.Count(); i++)
    {
        <tr>
            <td>
                @Html.HiddenFor(x => Model[i].Id)
                @Html.HiddenFor(x => Model[i].Name)
                @Html.CheckBoxFor(x => Model[i].Selected, Model[i].Name)
                @Html.DisplayFor(x => Model[i].Name)
            </td>
        </tr>            
    }
</table>